{
  "name": "theory-test-admission-reception-app",
  "description": "Reception App",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.11.1 <7.0"
  },
  "scripts": {
    "start": "node -r dotenv/config start.js",
    "clean": "gulp clean",
    "build-dist": "gulp build-dist",
    "build-asset-folder": "gulp create-assets-folder",
    "config": "node ./scripts/configure.js",
    "deconfig": "node ./scripts/deconfigure.js",
    "local": "node scripts/local",
    "test": "npm run unit",
    "integration": "gulp integration",
    "unit": "gulp unit",
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc lib test gulp app  \"./*.js\"",
    "prepush": "npm test && npm run lint && npm run check-security",
    "check-security": "git secrets --scan",
    "check-security-log": "git log -p | scanrepo"
