    "request": "^2.83.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.2",
    "serve-favicon": "2.4.2",
    "standard": "^11.0.0",
    "supertest": "^3.0.0",
    "sync-request": "^4.0.3",
    "uuid": "^3.2.1",
    "logger": "file:../logger"
