    "compression": "1.7.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "cross-spawn": "^5.0.0",
    "dotenv": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "4.15.2",
    "express-force-ssl": "^0.3.2",
    "express-paginate": "^0.3.0",
    "express-validator": "4.3.0",
